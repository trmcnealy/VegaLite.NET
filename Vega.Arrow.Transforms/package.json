{
    "name": "vega-arrow-transforms",
    "version": "0.0.1",
    "repository": "git@github.com:vega/trmcnealy/VegaLite.NET.git",
    "author": "Timothy R. McNealy <https://trmcnealy.github.io>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "clean": "npm run prebuild",
        "prebuild": "rimraf build && mkdir build",
        "build": "tsc --build tsconfig.json",
        "microbundle": "microbundle --format umd --no-sourcemap --external vega-lite --name vega-arrow-transforms --output build --entry \"build/index.js\"",
        "parcel": "parcel build --no-source-maps --out-dir build -o build/src/arrow-transform.js build/index.js",
        "_buildterser": "terser build/vega-arrow-transforms.js --ecma 2015 -c -m -o build/vega-arrow-transforms.min.js",
        "__postbuild": "npm run rollup && npm run buildterser",
        "rollup": "rollup -c rollup.config.js",
        "postbuild": "npm run rollup",
        "pretest": "npm prebuild && npm run rollup",
        "test": "tape 'test/**/*-test.js' && eslint index.js src test",
        "prepublishOnly": "npm test && npm build",
        "postpublish": "git push && git push --tags"
    },
    "dependencies": {
        "apache-arrow": "^0.16.0",
        "vega-typings": "^0.3.45",
        "vega-lite": "^4.10.1"
    },
    "peerdependencies": {
        "apache-arrow": "^0.16.0",
        "vega-lite": "^4.10.1"
    },
    "devDependencies": {
        "typescript": "^3.8.3",
        "parcel-bundler": "^1.12.4",
        "microbundle": "^0.11.0",
        "rimraf": "^3.0.2",
        "tape": "^4.13.0",
        "terser": "^4.6.3",
        "rollup": "^2.3.0",
        "rollup-plugin-closure-compiler-js": "^1.0.6",
        "rollup-plugin-glob-import": "^0.4.5",
        "@rollup/plugin-inject": "^4.0.1",
        "@rollup/plugin-typescript": "^4.0.0",
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1"
    },
    "main": "build/vega-arrow-transforms.js",
    "unpkg": "build/vega-arrow-transforms.min.js",
    "jsdelivr": "build/vega-arrow-transforms.min.js",
    "module": "build/index",
    "types": "build/index.d.ts",
    "source": "build/index.js"
}
